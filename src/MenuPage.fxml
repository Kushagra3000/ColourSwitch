<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="menuPage" prefHeight="600.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1024.0" layoutX="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/Background.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="133.0" fitWidth="130.0" layoutX="447.0" layoutY="309.0" onMouseClicked="#startNewGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/Play.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="222.0" layoutX="76.0" layoutY="477.0" onMouseClicked="#loadGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/loadingBtn.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="80.0" fitWidth="139.0" layoutX="773.0" layoutY="460.0" onMouseClicked="#ExitGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/exitbutton.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
